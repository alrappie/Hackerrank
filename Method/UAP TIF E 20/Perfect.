import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        //semangat !
        Scanner GG = new Scanner(System.in);
        int data = GG.nextInt();
        int temp=0;
        for (int i =1;i<data;i++){
            if(data%i==0){
                temp+=i;
            }
        }
        System.out.println(temp==data?"true":"false");
    }
}
